html,
body {
  height: 100%;
  width: 100%;
  margin: 0;
  margin: 0;
  padding: 0;
  overflow-x: hidden;
}
body {
  font-family: Areal, sans-serif;
  color:#fff;
  background:#191919;
}
a {
  text-decoration: none;
}
#header {
  box-shadow: black -2px 0px 10px 0px;
}

#mark {
  clip-path: inset(0);
  height: 100%;
  width: 20%;
}#mark::before {
  content: "";
  position: absolute;
  right: 0em;
  top: -4em;
  width: 11em;
  height: 10em;
  transform: rotate(-25deg);
  background: rgba(255,255,255,0.4);
}#mark::after {
  content: "";
  position: absolute;
  right: 0em;
  top: -4em;
  width: 14em;
  height: 10em;
  transform: rotate(-25deg);
  background: rgba(255,255,255,0.2);
}

#answer-container {
    border-radius: 13px;
}
#answer{
    overflow-wrap: break-word;
}

/* Resused styles
  ---------------------------------*/
.title {
  width: 100%;
  height: 2em;
  font-size: 1.5em;
  background: #00959a;
  padding-left: 0.4em;
  margin-bottom: 0.2em;
}
.item-text-title {
  font-size: 1.22em;
}
.item-text-description {
  font-size: 1.1em;
}
.overflowH {
  overflow: hidden;
}
.auto-scroll-y{
    overflow-y:auto;
}

/* Flex classes
  -------------------------------------------------------------*/
.flex {
  display: flex;
}
.inline-flex {
  display: inline-flex;
}
.row {
  flex-flow: row;
}
.wrap {
  flex-wrap: wrap;
}
.column {
  flex-flow: column;
}
.reverse {
  flex-flow: wrap-reverse;
}
.evenly {
  justify-content: space-evenly;
}
.between {
  justify-content: space-between;
}
.around {
